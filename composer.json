{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3.0",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "jfcherng/php-diff": "^6.12",
        "laravel/framework": "^11.32",
        "laravel/sanctum": "^4.20",
        "laravel/tinker": "^2.7",
        "phpmd/phpmd": "^2.13",
        "predis/predis": "^2.0",
        "sebastian/diff": "^5.1",
        "spatie/fork": "^1.2",
        "tightenco/tlint": "^9.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.1",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "josiasmontag/laravel-redis-mock": "^1.3",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.38",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^2.6",
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpcov": "^9.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "beyondcode/tinkerwell": "*"
    },
    "autoload": {
        "files": [
            "app/helpers.php",
            "packages/larawell/inspectation/src/Functions.php",
            "domain/DirtyCode/products.php",
            "domain/DirtyCode/shapes.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Domain\\": "domain/",
            "Larawell\\LaravelPlus\\": "packages/larawell/laravel-plus/src",
            "Larawell\\Inspectation\\": "packages/larawell/inspectation/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "tlint": [
            "tlint lint"
        ],
        "tlint:fix": [
            "tlint format"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
